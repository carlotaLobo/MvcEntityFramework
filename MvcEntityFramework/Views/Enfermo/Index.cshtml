@model List<Enfermo>

@Html.ActionLink("buscar por genero", "enfermosgenero", "enfermo", null, new { @class = "nav-link btn btn-outline-success my-2" })
@Html.ActionLink("insertar enfermo", "insert", "enfermo", null, new { @class = "nav-link btn btn-outline-success my-2" })

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Apellido</th>
            <th>Detalles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Enfermo e in Model)
        {
        <tr>
            <td>@e.Apellido</td>
            <td>
                @*@Html.ActionLink("Detalle","Detalle","Enfermo", new { id= @e.Inscripcion })*@
                <a asp-action="detalle" asp-controller="enfermo" asp-route-id="@e.Inscripcion">detalles</a>
            </td>
            <td>
                <a asp-action="eliminar" asp-controller="enfermo" asp-route-id="@e.Inscripcion">eliminar</a>
            </td>
            <td>
                <a asp-action="update" asp-controller="enfermo" asp-route-id="@e.Inscripcion">actualizar</a>
            </td>
        </tr>
        }
    </tbody>
</table>