@model List<Departamento> 

<h1>Departamentos</h1>

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Numero</th>
            <th>Nombre</th>
            <th>Localidad</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Departamento dept in Model)
        {
        <tr>
            <td>@dept.Numero</td>
            <td>@dept.Nombre</td>
            <td>@dept.Localidad</td>
        </tr>
        }
    </tbody>
</table>

